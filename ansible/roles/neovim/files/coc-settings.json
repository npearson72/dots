{
  "coc.preferences.jumpCommand": "tabe",
  "coc.preferences.rootPatterns": [
    "package.json",
    ".gitignore",
    ".env",
    ".git"
  ],
  "coc.source.emoji.filetypes": null,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.run": "onSave",
  "eslint.filetypes": ["javascript", "vue"],
  "explorer.file.child.template": "[selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1]",
  "explorer.file.column.indent.indentLine": true,
  "explorer.file.root.template": "[icon] [fullpath]",
  "explorer.file.showHiddenFiles": true,
  "explorer.filename.colored.enable": {
    "diagnosticError": true,
    "diagnosticWarning": false,
    "git": false
  },
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<2-LeftMouse>": "null",
    "l": "null",
    "E": "null",
    "v": "open:vsplit"
  },
  "explorer.openAction.relativePath": true,
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": true,
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "prettier.arrowParens": "avoid",
  "prettier.eslintIntegration": false, // Prettier throwing errors in typescript
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "snippets.extends": {
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "suggest.snippetIndicator": " ",
  "typescript.updateImportsOnFileMove.enable": false,
  "vetur.validation.template": false
}
